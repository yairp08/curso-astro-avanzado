---
import BaseHead from "@/components/BaseHead.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="es">
  <BaseHead title={title} />
  <body class="bg-dark-blue">
    <div class="py-6 max-md:mx-10">
      <main slot="main">
        <slot />
      </main>
      <footer></footer>
    </div>
  </body>
</html>
